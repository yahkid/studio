
rules_version = '2';

service cloud.firestore {
  match /databases/{database}/documents {
    // This rule allows anyone to read and write to any document.
    // WARNING: This is insecure and for development purposes only.
    // You MUST secure your database before launching to production.
    match /{document=**} {
      allow read, write: if true;
    }
  }
}
